<% @places.each do |place| %>
<div class="row">
  <div class="col-xs-10 col-xs-offset-1">
  	<div class="booyah-box"> 
	    <h1><%= place.name %></h1>
	    <i><%= place.address %></i><br /><br />
	    <p><%= place.description %></p><br />
  	</div>
  </div>
</div>
<% end %>




<div class="container-fluid">
	<%= yield %>
	<div class="row">
		<div class="col-xs-10 col-xs-offset-1">
			<div class="l-pagination">
			<%= will_paginate @places %>
			</div>
		</div>
	</div>
</div>